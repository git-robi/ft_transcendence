{

  "compilerOptions": {
    // File Layout
     "baseUrl": "./",
     "rootDir": "./src",
     "outDir": "./dist",
     "module": "commonjs",
     "target": "es2016",
     "strict": true,
     "esModuleInterop": true,
     "skipLibCheck": true,
	 "paths": {
	 	"prisma/client": ["node_modules/.prisma/client"]
	 }
  },

  "include": ["src/**/*.ts"],
  "exclude":  ["node_modules", "prisma"]
}
